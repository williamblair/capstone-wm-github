# rc file for initialization of capstone wm
# all paths are relative to the capstone wm executable
menuFile: ProgramMenu/testmenu.txt

# color of the title bar for the window
titleBarColor: 0x0000ff

# color of the border around the window
borderColor: 0xff00ff

# color of the font on the title bars
titleFontColor: 0xffffff

# color of the font on the task bar
taskbarFontColor: 0xff0000